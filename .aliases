alias pgstart='pg_ctl -D /usr/local/var/postgres -l /usr/local/var/postgres/server.log start'
alias pgstop='pg_ctl -D /usr/local/var/postgres stop -s -m fast'
alias mongostart='sudo mongod run --config /usr/local/etc/mongod.conf'
alias redisstart='launchctl load -w ~/Library/LaunchAgents/*.redis.plist'
alias redisstop='launchctl unload -w ~/Library/LaunchAgents/*.redis.plist'

alias coffee='/Users/dylan/node_modules/coffee-script/bin/coffee'
alias memcached='/usr/local/bin/memcached'

alias coffee_rails='coffee -o public/javascripts/ -wl app/scripts/*'
alias be='bundle exec'
alias gs='git status'
alias diffx='git diff --staged | gitx'
alias devlog='tail -f log/development.log'
alias testlog='tail -f log/test.log'
alias make="make -j4"

alias ..='cd ../'
alias ...='cd ../../'
alias ....='cd ../../../'
alias .....='cd ../../../../'
